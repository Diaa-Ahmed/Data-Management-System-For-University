<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.DepartmentViewController">
   <children>
      <TableView fx:id="departmentlist" prefHeight="600.0" prefWidth="800.0">
         <columns>
            <TableColumn fx:id="didcol" prefWidth="394.0" text="Department ID" />
            <TableColumn fx:id="dnamecol" prefWidth="405.0" text="Department Name" />
         </columns>
      </TableView>
      <VBox alignment="CENTER" layoutX="800.0" layoutY="155.0" prefHeight="291.0" prefWidth="100.0" spacing="30.0">
         <children>
            <Button fx:id="addbtn" mnemonicParsing="false" prefHeight="31.0" prefWidth="71.0" text="Add" />
            <Button fx:id="updatebtn" mnemonicParsing="false" text="Update" />
            <Button fx:id="deletebtn" mnemonicParsing="false" prefHeight="31.0" prefWidth="74.0" text="Delete" />
         </children>
      </VBox>
   </children>
</AnchorPane>
