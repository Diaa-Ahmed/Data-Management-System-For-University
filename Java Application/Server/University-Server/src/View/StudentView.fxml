<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.StudentViewController">
   <children>
      <TableView fx:id="studentlist" editable="true" prefHeight="600.0" prefWidth="800.0">
        <columns>
          <TableColumn fx:id="fnamecol" prefWidth="99.0" text="First Name" />
          <TableColumn fx:id="lnamecol" prefWidth="99.0" text="Last Name" />
            <TableColumn fx:id="levelcol" prefWidth="70.0" text="Level" />
            <TableColumn fx:id="gpacol" prefWidth="70.0" text="GPA" />
            <TableColumn fx:id="thourscol" prefWidth="75.0" text="Hours" />
            <TableColumn fx:id="phonecol" prefWidth="125.0" text="Phone" />
            <TableColumn fx:id="majorcol" prefWidth="75.0" text="Major" />
            <TableColumn fx:id="minorcol" prefWidth="75.0" text="Minor" />
            <TableColumn fx:id="emailcol" minWidth="0.0" prefWidth="225.0" text="Email" />
            <TableColumn fx:id="passcol" prefWidth="110.0" text="Password" />
            <TableColumn fx:id="citycol" prefWidth="100.0" text="City" />
            <TableColumn fx:id="streetcol" prefWidth="120.0" text="Street" />
            <TableColumn fx:id="buildingcol" prefWidth="75.0" text="Building " />
        </columns>
      </TableView>
      <VBox alignment="CENTER" layoutX="800.0" layoutY="155.0" prefHeight="291.0" prefWidth="100.0" spacing="30.0">
         <children>
            <Button fx:id="addbtn" mnemonicParsing="false" prefHeight="31.0" prefWidth="71.0" text="Add" />
            <Button fx:id="updatebtn" mnemonicParsing="false" text="Update" />
            <Button fx:id="deletebtn" mnemonicParsing="false" prefHeight="31.0" prefWidth="74.0" text="Delete" />
         </children>
      </VBox>
   </children>
</AnchorPane>
