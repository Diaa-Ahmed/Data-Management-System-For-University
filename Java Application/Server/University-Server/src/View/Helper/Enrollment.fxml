<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Helper.EnrollmentController">
   <children>
      <TableView fx:id="enrollmentlist" layoutY="47.0" prefHeight="598.0" prefWidth="808.0">
         <columns>
            <TableColumn fx:id="cnamecol" prefWidth="299.0" text="Course Name" />
            <TableColumn fx:id="credithourscol" prefWidth="142.0" text="Credit Hours" />
            <TableColumn fx:id="grade_col" minWidth="0.0" prefWidth="210.0" text="Grade" />
            <TableColumn fx:id="year" minWidth="0.0" prefWidth="156.0" text="Year" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="addbtn" layoutX="294.0" layoutY="652.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="191.0" text="Add Enrollment">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Label fx:id="student_name" alignment="CENTER" layoutX="14.0" layoutY="10.0" prefHeight="21.0" prefWidth="256.0" text="Student Name">
