      </Label>
      <Label layoutX="625.0" layoutY="10.0" prefHeight="21.0" prefWidth="48.0" text="GPA : ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="gpalabel" layoutX="690.0" layoutY="6.0" text="Label">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="deletebtn" layoutX="677.0" layoutY="652.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="86.0" text="Delete">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <ChoiceBox fx:id="yearcb" layoutX="297.0" layoutY="8.0" prefHeight="31.0" prefWidth="185.0" />
   </children>
</AnchorPane>
